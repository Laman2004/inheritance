# Animal class and inheritance
class animal:
  def __init__(self,name,age):
    self.name=name,
    self.age=age
  
  def info():
    pass

class dog(animal):
  def __init__(self,name,age,color):
    super().__init__(name,age)
    self.color=color

  def info(self):
    print(f"My dog name is {self.name} and he is {self.age} and {self.color}")

dog=dog('Toplan',5,'black')
print(dog.age)
dog.info()
